/*
* Icons
* 
*/



@font-face {
    font-family: 'icomoon';
    src:    url('fonts/icomoon.eot?z63gw5');
    src:    url('fonts/icomoon.eot?z63gw5#iefix') format('embedded-opentype'),
        url('fonts/icomoon.ttf?z63gw5') format('truetype'),
        url('fonts/icomoon.woff?z63gw5') format('woff'),
        url('fonts/icomoon.svg?z63gw5#icomoon') format('svg');
    font-weight: normal;
    font-style: normal;
}

[class^="icon-"], [class*=" icon-"]
    font-family: 'icomoon' !important
    speak: none
    font-style: normal
    font-weight: normal
    font-variant: normal
    text-transform: none
    line-height: 1
    -webkit-font-smoothing: antialiased
    -moz-osx-font-smoothing: grayscale

.icon-arrow
    &:before
        content: "\e900"

.icon-sad
    &:before
        content: "\e902"

.icon-premium
    &:before
        content: "\e901"

